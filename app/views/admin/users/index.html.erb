<%= link_to "Add User", new_admin_user_path %>

<% @users.each do |user| %>
  <p><%= link_to user.full_name, user_path(user) %> (<%= link_to "edit", edit_admin_user_path(user) %>) (<%= link_to "delete", admin_user_path(user), method: :delete, confirm: "You sure?" %>)</p>
<% end %>

<%= paginate @users %>